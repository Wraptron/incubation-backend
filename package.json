{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API server for Incubation Screen",
  "main": "dist/index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "dev:staging": "NODE_ENV=staging nodemon --exec ts-node src/index.ts",
    "dev:production": "NODE_ENV=production nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "build:staging": "NODE_ENV=staging tsc",
    "build:production": "NODE_ENV=production tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "start:staging": "NODE_ENV=staging node dist/index.js",
    "start:dev": "NODE_ENV=development node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "api",
    "backend",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@supabase/supabase-js": "^2.90.1",
    "@types/nodemailer": "^7.0.9",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "nodemailer": "^7.0.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.3",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
